---
pagination:
  data: collections
  size: 1
  alias: tag
permalink: /{{ tag }}/
layout: gallery
---

<div id="gallery">
{% set taglist = collections[ tag ]  %}
{% for post in taglist | reverse %}
<div><a href="{{ post.url }}"><img src="{{post.data.image}}" alt="{{ post.data.tite }}"></a></div>
{% endfor %}
</div>
